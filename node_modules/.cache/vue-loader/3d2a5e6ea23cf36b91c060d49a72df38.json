tfolios[this.selected]);\n      this.$emit(\"popupi\", this.selected);\n    },\n    popupRight() {\n      if (this.portfolios.length - 1 <= this.selected) {\n        return;\n      }\n      this.selected += 1;\n      this.$emit(\"popup\", this.portfolios[this.selected]);\n      this.$emit(\"popupi\", this.selected);\n    },\n    popupJump(i) {\n      this.selected = i;\n      this.$emit(\"popup\", this.portfolios[this.selected]);\n      this.$emit(\"popupi\", this.selected);\n    },\n  },\n};\n</script>\n\n<style scoped>\n.img-close {\n  width: unset;\n  min-width: unset;\n  max-width: unset;\n  min-height: unset;\n  max-height: unset;\n}\nsvg {\n  cursor: pointer;\n}\nimg {\n  cursor: pointer;\n  -khtml-user-select: none;\n  -o-user-select: none;\n  -moz-user-select: none;\n  -webkit-user-select: none;\n  user-select: none;\n  -webkit-user-drag: none;\n  -khtml-user-drag: none;\n  -moz-user-drag: none;\n  -o-user-drag: none;\n  user-drag: none;\n}\n.allportfolios-container {\n  width: 1280px;\n  height: 100%;\n}\n.allportfolios {\n  z-index: 2;\n  background: #fff;\n  position: fixed;\n  height: 100%;\n  width: 100%;\n  top: 0px;\n  left: 0px;\n  overflow: auto;\n}\n.allportfolios-header {\n  width: 100%;\n  padding: 60px 40px 0px 40px;\n  background: #fff;\n}\n.allportfolios-title {\n  text-align: left;\n  font-size: 12px;\n  line-height: 16px;\n  letter-spacing: 3px;\n  color: #384268;\n  opacity: 0.68;\n}\n.allportfolios-subtitle {\n  font-weight: bold;\n  font-size: 34px;\n  line-height: 40px;\n  color: #384268;\n}\n.allportfolios-div {\n  padding: 40px 40px;\n  background-color: #fff;\n  border-radius: 20px;\n  overflow: auto;\n  transition: width 1s;\n}\n.allportfolios-imgdes {\n  text-align: left;\n  width: 25%;\n}\n.heading5 {\n  width: 100%;\n}\n.body3 {\n  width: 100%;\n}\n.allportfolios-img {\n  min-width: 300px;\n  max-width: 700px;\n  min-height: 300px;\n  max-height: 700px;\n  border-radius: 5px;\n  object-fit: cover;\n}\n\n.businesspage-title-b {\n  